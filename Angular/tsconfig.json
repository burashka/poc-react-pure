{
	"compilerOptions": {
		"target": "es2016",
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [ "es2016", "dom" ],
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"typeRoots": [
			"node_modules/@types/"
		]
	},
	"exclude": [
		"node_modules"
	]
}